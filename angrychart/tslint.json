{
  "rules": {
    "class-name": true,
    "indent": [true, "spaces"],
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-trailing-whitespace": false,
    "no-var-keyword": true,
    "semicolon": false,
    "triple-equals": [true, "allow-null-check"],


    "variable-name": [true, "ban-keywords"],

    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]

  }
}